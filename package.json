{
  "name": "indy-poc1",
  "version": "0.0.1",
  "description": "My first Indy Agent PoC",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "debug": "tsc && node dist/index.js --inspect",
    "test": "jest",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "postinstall": "node-config-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduelias/indy-poc1.git"
  },
  "author": "Eduardo Elias Saleh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eduelias/indy-poc1/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "homepage": "https://github.com/eduelias/indy-poc1#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "cors": "^2.8.5",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "node-config-ts": "^2.2.4",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/socket.io": "^2.1.4",
    "aries-sdk-ts-beta": "../aries-sdk-ts/",
    "bs58": "^4.0.1",
    "express": "^4.17.1",
    "ffi": "^2.3.0",
    "libsodium-wrappers": "^0.7.6",
    "module-alias": "^2.2.2",
    "object-sizeof": "^1.5.2",
    "sleep": "^6.1.0",
    "socket.io": "^2.3.0",
    "zeromq": "^5.1.0"
  },
  "_moduleAliases": {
    "@Modules": "dist/Modules",
    "@Services": "dist/Services",
    "@Servers": "dist/Servers",
    "@Model": "dist/Model",
    "@Config": "Config"
  }
}
